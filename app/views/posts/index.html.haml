.container
  .posts-wrapper.row
    -@posts.each do |post|
      .post
        .post-head
          .post-caption
            = post.caption
        .post-image
          = link_to (image_tag post.image.url(:medium), class:'img-responsive'), post_path(post)
        .post-user
          %span.glyphicon.glyphicon-user{"aria-hidden" => "true"}
          Uploader name